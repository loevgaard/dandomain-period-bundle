{
    "name": "loevgaard/dandomain-period-bundle",
    "description": "Symfony bundle for automatically creating periods in Dandomain, which are commonly used in the creation of products",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": "^7.1",
        "loevgaard/dandomain-import-export": "^0.4.2",
        "symfony/console": "^3.0||^4.0",
        "symfony/framework-bundle": "^3.0||^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.5||^6.0",
        "symfony/phpunit-bridge": "^3.0||^4.0"
    },
    "autoload": {
        "psr-4": {
            "Loevgaard\\DandomainPeriodBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "./vendor/bin/php-cs-fixer fix --verbose --dry-run",
        "fix-style": "./vendor/bin/php-cs-fixer fix --verbose"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
